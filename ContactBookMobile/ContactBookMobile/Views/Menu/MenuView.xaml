<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ContactBookMobile.Views.Menu.MenuView"
            xmlns:views="clr-namespace:ContactBookMobile.Views"
             xmlns:pages="clr-namespace:ContactBookMobile.Views.Menu">
  <FlyoutPage.Flyout>
    <pages:MenuViewFlyout x:Name="FlyoutPage" />
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <views:DashboardView />
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>